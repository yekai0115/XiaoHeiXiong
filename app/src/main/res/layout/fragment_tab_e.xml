<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/xiaoheixiong_title_red" />

    <td.com.xiaoheixiong.views.overscrollview.OverScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/view_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/head_img"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="15dp"
                    android:src="@mipmap/head_icons" />

                <TextView
                    android:id="@+id/name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="姓名"
                    android:textColor="@color/black"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/dengji_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/red_btn"
                    android:padding="2dp"
                    android:text="等级"
                    android:textColor="@color/white"
                    android:textSize="16sp" />


                <ImageView
                    android:id="@+id/zhifu_img"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="10dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/z_icon" />

                <ImageView
                    android:id="@+id/weixin_img"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="5dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/w_icon" />

                <ImageView
                    android:id="@+id/shangjia_img"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="5dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/s_icon" />

                <ImageView
                    android:id="@+id/daili_img"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="5dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/d_icon" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/yunying_ll"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="@color/grays2" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:gravity="center_vertical"
                    android:text="运营中心"
                    android:textColor="@color/black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/grays" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/my_mechat_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/y_sh_icon"
                        android:gravity="center"
                        android:text="我的商家"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/daili_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/y_dl_icon"
                        android:gravity="center"
                        android:text="我的代理"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/tradingManagement_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/jygl_icon"
                        android:gravity="center"
                        android:text="交易管理"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/shouyi33"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/y_sy_icon"
                        android:gravity="center"
                        android:text="我的收益"
                        android:textColor="@color/black"
                        android:visibility="invisible"/>
                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/shanghu_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="@color/grays2" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:gravity="center_vertical"
                    android:text="商户中心"
                    android:textColor="@color/black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/grays" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/pay_code_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_skm_icon"
                        android:gravity="center"
                        android:text="收款码"
                        android:textColor="@color/black" />

                    <TextView

                        android:id="@+id/my_books_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_zb_icon"
                        android:gravity="center"
                        android:text="我的账本"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/sh_shouyi_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_sy_icon"
                        android:gravity="center"
                        android:text="我的收益"
                        android:textColor="@color/black"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/sh_yx_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_yx_icon"
                        android:gravity="center"
                        android:text="营销"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/fans_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_fs_icon"
                        android:gravity="center"
                        android:text="流量管理"
                        android:textColor="@color/black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/dpgl_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_dpgl_icon"
                        android:gravity="center"
                        android:text="店铺管理"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/my_shop_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_sp_icon"
                        android:gravity="center"
                        android:text="我的商品"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/sh_dpkj_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_dpkj_icon"
                        android:gravity="center"
                        android:text="店铺卡券"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/sh_dphb_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_dphb_icon"
                        android:gravity="center"
                        android:text="店铺红包"
                        android:textColor="@color/black" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/shouyi"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/y_sy_icon"
                        android:gravity="center"
                        android:text="我的收益"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/sh_yxtg_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_yxtg_icon"
                        android:gravity="center"
                        android:text="游戏推广"
                        android:textColor="@color/black"
                        android:visibility="invisible" />

                    <TextView
                        android:id="@+id/my_mvp_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_hy_icon"
                        android:gravity="center"
                        android:text="我的会员"
                        android:textColor="@color/black"
                        android:visibility="invisible" />

                    <TextView
                        android:id="@+id/sh_dt_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_dt_icon"
                        android:gravity="center"
                        android:text="我的动态"
                        android:textColor="@color/black"
                        android:visibility="invisible" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/grays2" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView63"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:gravity="center_vertical"
                    android:text="用户中心"
                    android:textColor="@color/black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/grays" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/yh_money_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/yh_qb_icon"
                        android:gravity="center"
                        android:text="我的钱包"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/yh_points_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/yh_jf_icon"
                        android:gravity="center"
                        android:text="我的积分"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/yh_zuji_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/yh_zj_icon"
                        android:gravity="center"
                        android:text="我的足迹"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/yh_guanzhu_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/yh_gz_icon"
                        android:gravity="center"
                        android:text="我的关注"
                        android:textColor="@color/black" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/yh_share_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/yh_fx_icon"
                        android:gravity="center"
                        android:text="分享"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/yh_toutiao_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/tt_icon"
                        android:gravity="center"
                        android:text="我的头条"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/yh_kq_tv"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/s_dpkj_icon"
                        android:gravity="center"
                        android:text="我的卡券"
                        android:textColor="@color/black" />

                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/phone_icon"
                        android:gravity="center"
                        android:text="我的商户"
                        android:textColor="@color/black"
                        android:visibility="invisible" />

                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/grays2" />


            <TextView
                android:id="@+id/mechat_register_tv"
                android:layout_width="250dp"
                android:layout_height="45dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="30dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/red_btn"
                android:gravity="center"
                android:text="申请成为商家"
                android:textColor="@color/white"
                android:textSize="16sp" />


        </LinearLayout>

    </td.com.xiaoheixiong.views.overscrollview.OverScrollView>

</LinearLayout>
